{
    "name": "dbh/easy-deploy-bundle",
    "type": "symfony-bundle",
    "description": "Only for Symfony 5.4, modify from EasyCorp/easy-deploy-bundle",
    "keywords": ["deploy", "deployment", "deployer"],
    "homepage": "https://github.com/h136799711/easy-deploy-bundle",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": ">=7.2.0",
        "symfony/console": "5.4.*",
        "symfony/dependency-injection": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/filesystem": "5.4.*",
        "symfony/http-foundation": "5.4.*",
        "symfony/http-kernel": "5.4.*",
        "symfony/polyfill-mbstring": "1.25.*",
        "symfony/process": "5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "EasyCorp\\Bundle\\EasyDeployBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "EasyCorp\\Bundle\\EasyDeployBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
